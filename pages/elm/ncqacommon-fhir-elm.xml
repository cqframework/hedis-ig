   <statements>
      <def name="Patient" context="Patient">
         <expression xsi:type="SingletonFrom">
            <operand dataType="fhir:Patient" xsi:type="Retrieve"/>
         </expression>
      </def>
      <def name="Is Enrolled in Institutional SNP" context="Patient" accessLevel="Public">
         <expression xsi:type="Exists">
            <operand xsi:type="Query">
               <source alias="SNP">
                  <expression dataType="fhir:Encounter" codeProperty="type" xsi:type="Retrieve">
                     <codes name="X Institutional SNP Value Set" xsi:type="ValueSetRef"/>
                  </expression>
               </source>
               <where xsi:type="And">
                  <operand xsi:type="In">
                     <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="status" scope="SNP" xsi:type="Property"/>
                     </operand>
                     <operand xsi:type="List">
                        <element valueType="t:String" value="in-progress" xsi:type="Literal"/>
                        <element valueType="t:String" value="finished" xsi:type="Literal"/>
                     </operand>
                  </operand>
                  <operand xsi:type="Overlaps">
                     <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="period" scope="SNP" xsi:type="Property"/>
                     </operand>
                     <operand name="Measurement Period" xsi:type="ParameterRef"/>
                  </operand>
               </where>
            </operand>
         </expression>
      </def>
      <def name="Is Living Long Term in Institution" context="Patient" accessLevel="Public">
         <expression xsi:type="Or">
            <operand xsi:type="Exists">
               <operand xsi:type="Query">
                  <source alias="DischargeLongTerm">
                     <expression dataType="fhir:Encounter" codeProperty="type" xsi:type="Retrieve">
                        <codes name="Encounter Inpatient" xsi:type="ValueSetRef"/>
                     </expression>
                  </source>
                  <where xsi:type="And">
                     <operand xsi:type="And">
                        <operand xsi:type="In">
                           <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="status" scope="DischargeLongTerm" xsi:type="Property"/>
                           </operand>
                           <operand xsi:type="List">
                              <element valueType="t:String" value="in-progress" xsi:type="Literal"/>
                              <element valueType="t:String" value="finished" xsi:type="Literal"/>
                           </operand>
                        </operand>
                        <operand xsi:type="Or">
                           <operand xsi:type="Equivalent">
                              <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="dischargeDisposition" xsi:type="Property">
                                    <source path="hospitalization" scope="DischargeLongTerm" xsi:type="Property"/>
                                 </operand>
                              </operand>
                              <operand xsi:type="ToConcept">
                                 <operand name="Patient transfer, to another health care facility (procedure)" xsi:type="CodeRef"/>
                              </operand>
                           </operand>
                           <operand xsi:type="Equivalent">
                              <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="dischargeDisposition" xsi:type="Property">
                                    <source path="hospitalization" scope="DischargeLongTerm" xsi:type="Property"/>
                                 </operand>
                              </operand>
                              <operand xsi:type="ToConcept">
                                 <operand name="Referral to long term care service (procedure)" xsi:type="CodeRef"/>
                              </operand>
                           </operand>
                        </operand>
                     </operand>
                     <operand xsi:type="In">
                        <operand xsi:type="End">
                           <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="period" scope="DischargeLongTerm" xsi:type="Property"/>
                           </operand>
                        </operand>
                        <operand name="Measurement Period" xsi:type="ParameterRef"/>
                     </operand>
                  </where>
               </operand>
            </operand>
            <operand xsi:type="Exists">
               <operand xsi:type="Query">
                  <source alias="LongTermOrder">
                     <expression dataType="fhir:Task" codeProperty="code" xsi:type="Retrieve">
                        <codes name="Care Services in Long-Term Residential Facility" xsi:type="ValueSetRef"/>
                     </expression>
                  </source>
                  <where xsi:type="And">
                     <operand xsi:type="Equal">
                        <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                           <operand path="intent" scope="LongTermOrder" xsi:type="Property"/>
                        </operand>
                        <operand valueType="t:String" value="order" xsi:type="Literal"/>
                     </operand>
                     <operand xsi:type="Or">
                        <operand xsi:type="In">
                           <operand name="ToDateTime" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="authoredOn" scope="LongTermOrder" xsi:type="Property"/>
                           </operand>
                           <operand name="Measurement Period" xsi:type="ParameterRef"/>
                        </operand>
                        <operand xsi:type="Overlaps">
                           <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="executionPeriod" scope="LongTermOrder" xsi:type="Property"/>
                           </operand>
                           <operand name="Measurement Period" xsi:type="ParameterRef"/>
                        </operand>
                     </operand>
                  </where>
               </operand>
            </operand>
         </expression>
      </def>
      <def name="Has Hospice" context="Patient" accessLevel="Public">
         <expression xsi:type="Or">
            <operand xsi:type="Exists">
               <operand xsi:type="Query">
                  <source alias="DischargeHospice">
                     <expression dataType="fhir:Encounter" codeProperty="type" xsi:type="Retrieve">
                        <codes name="Encounter Inpatient" xsi:type="ValueSetRef"/>
                     </expression>
                  </source>
                  <where xsi:type="And">
                     <operand xsi:type="And">
                        <operand xsi:type="In">
                           <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="status" scope="DischargeHospice" xsi:type="Property"/>
                           </operand>
                           <operand xsi:type="List">
                              <element valueType="t:String" value="in-progress" xsi:type="Literal"/>
                              <element valueType="t:String" value="finished" xsi:type="Literal"/>
                           </operand>
                        </operand>
                        <operand xsi:type="Or">
                           <operand xsi:type="Equivalent">
                              <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="dischargeDisposition" xsi:type="Property">
                                    <source path="hospitalization" scope="DischargeHospice" xsi:type="Property"/>
                                 </operand>
                              </operand>
                              <operand xsi:type="ToConcept">
                                 <operand name="Discharge to home for hospice care (procedure)" xsi:type="CodeRef"/>
                              </operand>
                           </operand>
                           <operand xsi:type="Equivalent">
                              <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="dischargeDisposition" xsi:type="Property">
                                    <source path="hospitalization" scope="DischargeHospice" xsi:type="Property"/>
                                 </operand>
                              </operand>
                              <operand xsi:type="ToConcept">
                                 <operand name="Discharge to healthcare facility for hospice care (procedure)" xsi:type="CodeRef"/>
                              </operand>
                           </operand>
                        </operand>
                     </operand>
                     <operand xsi:type="In">
                        <operand xsi:type="End">
                           <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="period" scope="DischargeHospice" xsi:type="Property"/>
                           </operand>
                        </operand>
                        <operand name="Measurement Period" xsi:type="ParameterRef"/>
                     </operand>
                  </where>
               </operand>
            </operand>
            <operand xsi:type="Exists">
               <operand xsi:type="Query">
                  <source alias="HospiceTask">
                     <expression dataType="fhir:Task" codeProperty="code" xsi:type="Retrieve">
                        <codes name="Hospice care ambulatory" xsi:type="ValueSetRef"/>
                     </expression>
                  </source>
                  <where xsi:type="And">
                     <operand xsi:type="Equal">
                        <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                           <operand path="intent" scope="HospiceTask" xsi:type="Property"/>
                        </operand>
                        <operand valueType="t:String" value="order" xsi:type="Literal"/>
                     </operand>
                     <operand xsi:type="Or">
                        <operand xsi:type="In">
                           <operand name="ToDateTime" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="authoredOn" scope="HospiceTask" xsi:type="Property"/>
                           </operand>
                           <operand name="Measurement Period" xsi:type="ParameterRef"/>
                        </operand>
                        <operand xsi:type="Overlaps">
                           <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="executionPeriod" scope="HospiceTask" xsi:type="Property"/>
                           </operand>
                           <operand name="Measurement Period" xsi:type="ParameterRef"/>
                        </operand>
                     </operand>
                  </where>
               </operand>
            </operand>
         </expression>
      </def>
      <def name="ToDate" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression xsi:type="DateTime">
            <year precision="Year" xsi:type="DateTimeComponentFrom">
               <operand name="Value" xsi:type="OperandRef"/>
            </year>
            <month precision="Month" xsi:type="DateTimeComponentFrom">
               <operand name="Value" xsi:type="OperandRef"/>
            </month>
            <day precision="Day" xsi:type="DateTimeComponentFrom">
               <operand name="Value" xsi:type="OperandRef"/>
            </day>
            <hour valueType="t:Integer" value="0" xsi:type="Literal"/>
            <minute valueType="t:Integer" value="0" xsi:type="Literal"/>
            <second valueType="t:Integer" value="0" xsi:type="Literal"/>
            <millisecond valueType="t:Integer" value="0" xsi:type="Literal"/>
            <timezoneOffset xsi:type="TimezoneFrom">
               <operand name="Value" xsi:type="OperandRef"/>
            </timezoneOffset>
         </expression>
         <operand name="Value">
            <operandTypeSpecifier name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </operand>
      </def>
      <def name="CalendarAgeInDaysAt" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression precision="Day" xsi:type="DurationBetween">
            <operand name="ToDate" xsi:type="FunctionRef">
               <operand name="BirthDateTime" xsi:type="OperandRef"/>
            </operand>
            <operand name="ToDate" xsi:type="FunctionRef">
               <operand name="AsOf" xsi:type="OperandRef"/>
            </operand>
         </expression>
         <operand name="BirthDateTime">
            <operandTypeSpecifier name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </operand>
         <operand name="AsOf">
            <operandTypeSpecifier name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </operand>
      </def>
      <def name="CalendarAgeInDays" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression name="CalendarAgeInDaysAt" xsi:type="FunctionRef">
            <operand name="BirthDateTime" xsi:type="OperandRef"/>
            <operand xsi:type="ToDateTime">
               <operand xsi:type="Today"/>
            </operand>
         </expression>
         <operand name="BirthDateTime">
            <operandTypeSpecifier name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </operand>
      </def>
      <def name="CalendarAgeInMonthsAt" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression precision="Month" xsi:type="DurationBetween">
            <operand name="ToDate" xsi:type="FunctionRef">
               <operand name="BirthDateTime" xsi:type="OperandRef"/>
            </operand>
            <operand name="ToDate" xsi:type="FunctionRef">
               <operand name="AsOf" xsi:type="OperandRef"/>
            </operand>
         </expression>
         <operand name="BirthDateTime">
            <operandTypeSpecifier name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </operand>
         <operand name="AsOf">
            <operandTypeSpecifier name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </operand>
      </def>
      <def name="CalendarAgeInMonths" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression name="CalendarAgeInMonthsAt" xsi:type="FunctionRef">
            <operand name="BirthDateTime" xsi:type="OperandRef"/>
            <operand xsi:type="ToDateTime">
               <operand xsi:type="Today"/>
            </operand>
         </expression>
         <operand name="BirthDateTime">
            <operandTypeSpecifier name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </operand>
      </def>
      <def name="LengthInDays" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression precision="Day" xsi:type="DifferenceBetween">
            <operand xsi:type="Start">
               <operand name="Value" xsi:type="OperandRef"/>
            </operand>
            <operand xsi:type="End">
               <operand name="Value" xsi:type="OperandRef"/>
            </operand>
         </expression>
         <operand name="Value">
            <operandTypeSpecifier xsi:type="IntervalTypeSpecifier">
               <pointType name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
            </operandTypeSpecifier>
         </operand>
      </def>
      <def name="CalendarAgeInYearsAt" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression precision="Year" xsi:type="DurationBetween">
            <operand name="ToDate" xsi:type="FunctionRef">
               <operand name="BirthDateTime" xsi:type="OperandRef"/>
            </operand>
            <operand name="ToDate" xsi:type="FunctionRef">
               <operand name="AsOf" xsi:type="OperandRef"/>
            </operand>
         </expression>
         <operand name="BirthDateTime">
            <operandTypeSpecifier name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </operand>
         <operand name="AsOf">
            <operandTypeSpecifier name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </operand>
      </def>
      <def name="CalendarAgeInYears" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression name="CalendarAgeInYearsAt" xsi:type="FunctionRef">
            <operand name="BirthDateTime" xsi:type="OperandRef"/>
            <operand xsi:type="ToDateTime">
               <operand xsi:type="Today"/>
            </operand>
         </expression>
         <operand name="BirthDateTime">
            <operandTypeSpecifier name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </operand>
      </def>
   </statements>
</library>
